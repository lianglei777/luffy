{
"presets": [
    [
        "@babel/preset-env",
        {
            "targets": {
              // 大于相关浏览器版本无需用到 preset-env
            "edge": 17,
            "firefox": 60,
            "chrome": 67,
            "safari": 11,
            // 兼容到android4 ios6
            "browsers": ["Android >= 4.0", "ios >= 6"]
            }
       }
   ],
    "@babel/preset-react"
],
"plugins": [
    [
      "import", { "libraryName": "antd-mobile", "style": "css" }] // `style: true` 会加载 less 文件
   ]
}